# Flowstate Backend Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_PATH=./data/flowstate.db

# CORS
CORS_ORIGIN=http://localhost:5173

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here
